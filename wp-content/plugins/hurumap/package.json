{
  "name": "@codeforafrica/hurumap-wp-plugin",
  "version": "0.1.0",
  "main": "build/index.js",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@storybook/addon-a11y": "5.3.2",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-docs": "5.3.2",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-storyshots": "5.3.2",
    "@storybook/addon-storysource": "5.3.2",
    "@storybook/addon-viewport": "5.3.2",
    "@storybook/addons": "^5.3.21",
    "@storybook/react": "^5.3.21",
    "@storybook/storybook-deployer": "^2.8.6",
    "@wordpress/scripts": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.4.0",
    "prettier": "^1.18.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "lint": "yarn eslint --fix  './src'",
    "start:storybook": "start-storybook -s ./",
    "start": "concurrently \"yarn start:definitions\" \"yarn start:sections\"  \"yarn start:blocks\"",
    "start:blocks": "wp-scripts start ./src/FlourishBlock/index.js ./src/HURUmapBlock/index.js ./src/SectionRowChartBlock/index.js ./src/FeaturedData/index.js ./src/CardBlock/index.js ./src/IndicatorBlock/index.js ./src/SectionRowBlock/index.js ./src/SectionBlock/index.js --output-path=build/blocks",
    "start:definitions": "wp-scripts start ./src/ChartDefinition/index.js --output-path=build/definitions",
    "start:sections": "wp-scripts start ./src/ChartsSection/index.js --output-path=build/section",
    "build": "yarn build:definitions && yarn build:sections && yarn build:blocks",
    "build:definitions": "wp-scripts build ./src/ChartDefinition/index.js --output-path=build/definitions",
    "build:sections": "wp-scripts build ./src/ChartsSection/index.js --output-path=build/section",
    "build:blocks": "wp-scripts build ./src/FlourishBlock/index.js ./src/HURUmapBlock/index.js ./src/SectionRowChartBlock/index.js ./src/FeaturedData/index.js ./src/CardBlock/index.js ./src/IndicatorBlock/index.js ./src/SectionRowBlock/index.js ./src/SectionBlock/index.js --output-path=build/blocks"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@hurumap-ui/content": "^0.3.5-alpha.40",
    "@hurumap-ui/core": "^0.3.5-alpha.40",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.5.1",
    "@mikecousins/react-pdf": "^5.5.0",
    "@reach/router": "^1.3.4",
    "@types/react": "^16.9.49",
    "@wordpress/block-editor": "^3.11.0",
    "@wordpress/blocks": "^6.22.0",
    "@wordpress/components": "^9.9.0",
    "@wordpress/data": "^4.23.1",
    "@wordpress/editor": "^9.22.0",
    "@wordpress/element": "^2.17.1",
    "@wordpress/i18n": "^3.15.0",
    "@wordpress/url": "^2.18.0",
    "apollo-boost": "^0.4.9",
    "apollo-client": "^2.6.10",
    "apollo-utilities": "^1.3.4",
    "classnames": "^2.2.6",
    "formik": "^2.1.5",
    "graphql": "^14.7.0",
    "graphql-tag": "^2.11.0",
    "lodash": "^4.17.20",
    "pluralize": "^8.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-ace": "^8.1.0",
    "react-dom": "^16.13.1",
    "react-dropzone": "^10.2.2",
    "react-resize-detector": "^4.2.3",
    "react-select": "^3.1.0",
    "react-transition-group": "^4.4.1",
    "react-with-direction": "^1.3.1",
    "shortid": "^2.2.15",
    "victory": "^35.0.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "yarn lint",
      "git add"
    ]
  }
}
